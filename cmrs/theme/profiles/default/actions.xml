<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="portal_tabs" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="plone_setup" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Site Setup</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/plone_control_panel</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="linkedin" meta_type="CMF Action" i18n:domain="cmrs.theme">
   <property name="title" i18n:translate="">LinkedIn</property>
   <property name="description" i18n:translate="">Follow us on LinkedIn</property>
   <property name="url_expr">string:http://www.linkedin.com/profile/view?id=21440423&trk=nav_responsive_tab_profile</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="facebook" meta_type="CMF Action" i18n:domain="cmrs.theme">
   <property name="title" i18n:translate="">Facebook</property>
   <property name="description" i18n:translate="">Follow us on Facebook</property>
   <property name="url_expr">string:https://www.facebook.com/groups/37897018362</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
